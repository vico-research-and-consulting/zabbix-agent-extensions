    <date>2020-12-31T16:08:15Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Custom - Service - NGINX Logstats</template>
            <name>Custom - Service - NGINX Logstats</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Service - NGINX - Cache</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Configured Logstat VHost</name>
                    <key>generic.discovery[nginx]</key>
                    <delay>3600</delay>
                    <description>nginx vhosts which have been configured to be have their cache statistics monitored by logstats</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>NGINX Cache Bypass {#NAME}</name>
                            <type>TRAP</type>
                            <key>nginx.cache.bypass[{#NAME}]</key>
                            <delay>0</delay>
                            <history>14d</history>
                            <trends>0</trends>
                            <status>DISABLED</status>
                            <units>req/sec</units>
                            <applications>
                                <application>
                                    <name>Service - NGINX - Cache</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <parameters>
                                        <parameter/>
                                    </parameters>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>NGINX Cache Expires {#NAME}</name>

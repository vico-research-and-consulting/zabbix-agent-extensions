<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>6.4</version>
    <template_groups>
        <template_group>
            <uuid>7df96b18c230490a9a0a9e2307226338</uuid>
            <name>Templates</name>
        </template_group>
    </template_groups>
    <templates>
        <template>
            <uuid>3555b6b4c0db45da830ff1664f50edc0</uuid>
            <template>Custom - Service - Springboot</template>
            <name>Custom - Service - Springboot</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <discovery_rules>
                <discovery_rule>
                    <uuid>0f87215c045f4bbe929bddaffd976415</uuid>
                    <name>Metric http.server.requests by uri</name>
                    <type>ZABBIX_ACTIVE</type>
                    <key>springboot.discovery[{$SPRINGBOOT_BASE_URI}/metrics/http.server.requests,uri]</key>
                    <delay>10m</delay>
                    <lifetime>2d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <uuid>7540a430894c4327bdbeca9d6af6bb15</uuid>
                            <name>Performance HTTP {#URI} Data</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>springboot.get[{$SPRINGBOOT_BASE_URI}/metrics/http.server.requests,tag=uri:{#URI}]</key>
                            <delay>3m</delay>
                            <history>2h</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <request_method>POST</request_method>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Springboot Http Request Performance</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>c8e9c1eecd2e44f19f456435f7a624dd</uuid>
                            <name>{#URI} Average Request Time</name>
                            <type>CALCULATED</type>
                            <key>springboot.uri[{#URI},average_responsetime]</key>
                            <history>14d</history>
                            <trends>90d</trends>
                            <value_type>FLOAT</value_type>
                            <units>s</units>
                            <params>last(//springboot.uri[{#URI},time])/last(//springboot.uri[{#URI},requests])</params>
                            <request_method>POST</request_method>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Springboot Http Request Performance</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>8c73a1e5488744728bb9eab548642f46</uuid>
                            <name>{#URI} Requests</name>
                            <type>DEPENDENT</type>
                            <key>springboot.uri[{#URI},requests]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <units>req</units>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$['COUNT']</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>SIMPLE_CHANGE</type>
                                    <parameters>
                                        <parameter/>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>springboot.get[{$SPRINGBOOT_BASE_URI}/metrics/http.server.requests,tag=uri:{#URI}]</key>
                            </master_item>
                            <request_method>POST</request_method>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Springboot Http Request Performance</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>1dbfd317e7ec4d3cb8a9e6b69283bfd0</uuid>
                            <name>{#URI} Requests/sec</name>
                            <type>DEPENDENT</type>
                            <key>springboot.uri[{#URI},requests_per_second]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <value_type>FLOAT</value_type>
                            <units>req/s</units>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$['COUNT']</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <parameters>
                                        <parameter/>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>springboot.get[{$SPRINGBOOT_BASE_URI}/metrics/http.server.requests,tag=uri:{#URI}]</key>
                            </master_item>
                            <request_method>POST</request_method>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Springboot Http Request Performance</value>
                                </tag>
                            </tags>
                        </item_prototype>
                        <item_prototype>
                            <uuid>70c88b61bc9342cab98eff772da1f1f2</uuid>
                            <name>{#URI} Time</name>
                            <type>DEPENDENT</type>
                            <key>springboot.uri[{#URI},time]</key>
                            <delay>0</delay>
                            <history>7d</history>
                            <trends>90d</trends>
                            <value_type>FLOAT</value_type>
                            <units>s</units>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$['TOTAL_TIME']</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>SIMPLE_CHANGE</type>
                                    <parameters>
                                        <parameter/>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>springboot.get[{$SPRINGBOOT_BASE_URI}/metrics/http.server.requests,tag=uri:{#URI}]</key>
                            </master_item>
                            <request_method>POST</request_method>
                            <tags>
                                <tag>
                                    <tag>Application</tag>
                                    <value>Springboot Http Request Performance</value>
                                </tag>
                            </tags>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <uuid>4f2a1352f17a477ab733768290fc6ead</uuid>
                            <name>Performance HTTP {#URI}</name>
                            <height>300</height>
                            <ymin_type_1>FIXED</ymin_type_1>
                            <graph_items>
                                <graph_item>
                                    <color>F63100</color>
                                    <yaxisside>RIGHT</yaxisside>
                                    <calc_fnc>MAX</calc_fnc>
                                    <item>
                                        <host>Custom - Service - Springboot</host>
                                        <key>springboot.uri[{#URI},requests_per_second]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>2774A4</color>
                                    <calc_fnc>MAX</calc_fnc>
                                    <item>
                                        <host>Custom - Service - Springboot</host>
                                        <key>springboot.uri[{#URI},average_responsetime]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                    <request_method>POST</request_method>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$SPRINGBOOT_BASE_URI}</macro>
                    <value>http://127.0.0.1:6001/actuator</value>
                </macro>
            </macros>
            <dashboards>
                <dashboard>
                    <uuid>421e7aa4afd54ba288541f4cd64c83de</uuid>
                    <name>Custom - Service - Springboot - HTTP Performance</name>
                    <pages>
                        <page>
                            <widgets>
                                <widget>
                                    <type>graphprototype</type>
                                    <width>24</width>
                                    <height>7</height>
                                    <fields>
                                        <field>
                                            <type>INTEGER</type>
                                            <name>columns</name>
                                            <value>1</value>
                                        </field>
                                        <field>
                                            <type>GRAPH_PROTOTYPE</type>
                                            <name>graphid</name>
                                            <value>
                                                <host>Custom - Service - Springboot</host>
                                                <name>Performance HTTP {#URI}</name>
                                            </value>
                                        </field>
                                        <field>
                                            <type>INTEGER</type>
                                            <name>rows</name>
                                            <value>1</value>
                                        </field>
                                        <field>
                                            <type>INTEGER</type>
                                            <name>source_type</name>
                                            <value>2</value>
                                        </field>
                                    </fields>
                                </widget>
                            </widgets>
                        </page>
                    </pages>
                </dashboard>
            </dashboards>
        </template>
    </templates>
</zabbix_export>
